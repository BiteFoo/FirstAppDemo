        <item>rm -r /data/data/app-lib/*kingroot*</item>
        <item>rm -r /data/data/app-lib/*kingstudio*</item>
        <item>rm -r -f /data/data-lib/king</item>
        <item>am force-stop com.kingroot.kinguser</item>
        <item>/system/bin/chattr /system/bin/.krsh</item>
        <item>rm /system/bin/.krsh</item>
        <item>/system/bin/busybox chattr /system/bin/sutemp</item>
        <item>rm /system/bin/sutemp</item>
        <item>/system/bin/busybox chattr /system/xbin/sutemp</item>
        <item>rm /system/xbin/sutemp</item>
        <item>echo Staging SuperSU</item>
        <item>cat /system/bin/toolbox > /system/toolbox</item>
        <item>chmod 0755 /system/toolbox</item>
        <item>/system/bin/busybox chattr -ia /system/xbin/daemonsu</item>
        <item>rm /system/xbin/daemonsu</item>
        <item>/system/bin/busybox chattr -ia /system/xbin/supolicy</item>
        <item>rm /system/xbin/supolicy</item>
        <item>/system/bin/busybox chattr -ia /system/xbin/sugote</item>
        <item>rm /system/xbin/sugote</item>
        <item>/system/bin/busybox chattr -ia /system/xbin/sugote-mksh</item>
        <item>rm /system/xbin/sugote-mksh</item>
        <item>cat myfolder/suver >/system/xbin/daemonsu</item>
        <item>chmod 0755 /system/xbin/daemonsu</item>
        <item>chown 0.0 /system/xbin/daemonsu</item>
        <item>/system/bin/busybox chattr -ia /system/xbin/su</item>
        <item>rm /system/xbin/su</item>
        <item>cat myfolder/suver >/system/xbin/su</item>
        <item>chmod 06755 /system/xbin/su</item>
        <item>chown 0.0 /system/xbin/su</item>
        <item>/system/bin/busybox chcon u:object_r:system_file:s0 /system/xbin/su</item>
        <item>/system/bin/busybox chattr -ia /system/sbin/su</item>
        <item>rm /system/sbin/su</item>
        <item>/system/bin/busybox chattr -ia /system/bin/su</item>
        <item>rm /system/bin/su</item>
        <item>/system/bin/busybox ln -s /system/xbin/su /system/bin/su</item>
        <item>/system/bin/busybox ln -s /syste/xbin/su /system/sbin/su</item>
        <item>/system/xbin/daemonsu -d</item>
